{
  "name": "cc-chats",
  "version": "0.2.0",
  "description": "Browse and search your Claude Code chat history",
  "license": "MIT",
  "keywords": ["claude", "claude-code", "chat", "history", "browser"],
  "bin": {
    "cc-chats": "./bin/cc-chats.mjs"
  },
  "files": [
    "bin/",
    ".next/",
    "!.next/cache",
    "!.next/trace",
    "!.next/types",
    "!.next/diagnostics"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepublishOnly": "npm run build && rm -rf .next/cache"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.38",
    "better-sqlite3": "^12.4.6",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.0"
  }
}
